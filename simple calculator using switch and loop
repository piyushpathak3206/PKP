#include <stdio.h>
int main(){
float c,b;
printf("enter two number for calculations: \n");
scanf("%f %f",&c,&b);
char n;
printf("enter operation by sign you want to do ");
scanf(" %c",&n);
int a = (int)n;
switch(a)
 {case (int)'+':
  printf("= %f",c+b);
  break;
 case (int)'-':
  printf("= %f",c-b);
   break;
 case (int)'*':
   printf("= %f",c*b);
    break;
 case (int)'/':
  printf("= %f",c/b);
   break;}

return 0;}
